a = 20
b = 10


if ( a == b ):
   print("a is equal to b")
else:
   print("a is not equal to b")

if ( a != b ):
   print("a is not equal to b")
else:
   print("a is equal to b")


if ( a <= b ):
   print("a is less than or equal to b") 
else:
   print("a is not less than or equal to b")

if ( a >= b ):
   print("a is greater than or equal to b")
else:
   print(" a is not greater than or equal to b")
